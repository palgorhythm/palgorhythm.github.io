{
  "name": "fractal-polyrhythms",
  "version": "1.0.0",
  "description": "fractal-polyrhythms",
  "main": "./js",
  "scripts": {
    "start": "budo . -d --serve bundle.js --live",
    "build": "browserify . -o bundle.js -g uglifyify",
    "test": "browserify test.js | smokestack | tap-spec",
    "dist": "mkdir -p dist && bash -c 'cp *.{js,html,css} dist'",
    "push": "gh-pages -d dist",
    "deploy": "npm run build && npm run dist && npm run push"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/palgorhythm/fractal-polyrhythms"
  },
  "author": "palgorhythm",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/palgorhythm/fractal-polyrhythms/issues"
  },
  "homepage": "https://github.com/palgorhythm/fractal-polyrhythms",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "form-to-object": "^5.1.0",
    "is-empty": "^1.2.0",
    "ismobilejs": "^0.4.1",
    "iterate-function": "^1.1.1",
    "jquery": "^3.4.1",
    "lodash": "^4.17.19",
    "lodash.range": "^3.2.0",
    "query-string": "^4.3.4",
    "startaudiocontext": "^1.2.1",
    "test": "^0.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^10.0.0",
    "budo": "^4.2.2",
    "gh-pages": "^0.3.0",
    "smokestack": "^3.3.0",
    "tap-spec": "^4.0.2",
    "tape": "^4.0.0",
    "uglifyify": "^3.0.4",
    "watchify": "^3.1.0"
  }
}
